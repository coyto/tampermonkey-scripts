{
  "name": "tamper-monkey-scripts",
  "version": "1.0.0",
  "description": "Collection of Tampermonkey scripts",
  "scripts": {
    "build": "node build.js",
    "dev": "concurrently \"chokidar build.js ./src/* -c 'node build.js'\" \"node server.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coyto/tampermonkey-scripts.git"
  },
  "keywords": [
    "tampermonkey",
    "scripts"
  ],
  "author": "coyto",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/coyto/tampermonkey-scripts/issues"
  },
  "packageManager": "pnpm@10.15.1",
  "dependencies": {
    "concurrently": "^9.2.1",
    "express": "^5.1.0",
    "chokidar-cli": "^3.0.0"
  }
}
